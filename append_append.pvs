append_append[T : TYPE+]: THEORY
  BEGIN 
    append_list(L : list[list[T]]) : RECURSIVE list[T] =
    IF null?(L) THEN (::)
    ELSE append(car(L), append_list(cdr(L)))
    ENDIF
    MEASURE length(L)
    
  END append_append
